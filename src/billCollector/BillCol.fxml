                  <Font name="System Bold" size="36.0" />
               </font>
            </Label>
            <Label layoutX="127.0" layoutY="113.0" text="Mobile No.">
               <font>
                  <Font size="24.0" />
               </font>
            </Label>
            <TextField fx:id="txtAmount" layoutX="208.0" layoutY="471.0" promptText="Total Amount" />
            <Label layoutX="146.0" layoutY="189.0" text="Date of bill">
